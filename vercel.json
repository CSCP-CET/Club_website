{
  "version": 2,
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm -r build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/api/assets/$1"
    },
    {
      "source": "/((?!api/|assets/).*)",
      "destination": "/index.html"
    }
  ]
}
