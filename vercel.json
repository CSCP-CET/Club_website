{
  "version": 2,
  "installCommand": "pnpm install --frozen-lockfile",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/server.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/backend/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}